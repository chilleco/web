[MASTER]
jobs=2

[MESSAGES CONTROL]
disable=unsubscriptable-object,
        unused-argument,
        no-name-in-module,
        duplicate-code,
        invalid-name

[FORMAT]
max-line-length=88

[MISCELLANEOUS]
notes=BUG,UGLY # TODO,FIXME

[BASIC]
docstring-min-length=-1
no-docstring-rgx=[(__.*__)(Type)]

[DESIGN]
max-args=8
max-attributes=10
min-public-methods=0
